{
  "type": 1,
  "name": "js.vp8.medooze.config.json %ts",
  "grids": [
    {
      "type": "local",
      "url": "http://localhost:4444/wd/hub"
    }
  ],
  "tests": [
    {
      "name": "JS Medooze Simulcast VP8 %ts",
